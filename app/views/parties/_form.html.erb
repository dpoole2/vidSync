<%= form_with(model: party) do |form| %>

  <div class="row">
    <div class="col-md-2">
      <%= form.label :party_name %>
    </div>
    <div class="col-md-4">
      <%= form.text_field :name, class: "form-control" %>
    </div>
  </div><br>

  <div class="row">
   <div class="col-md-2">
    <%= form.label :video_id %>
   </div>
    <div class="col-md-4">
      <%= form.text_field :yt_video_id, class: "form-control" %>
    </div>
  </div>

  <div class="actions">
    <%= form.submit class: 'btn btn-primary' %>
  </div>
<% end %>
